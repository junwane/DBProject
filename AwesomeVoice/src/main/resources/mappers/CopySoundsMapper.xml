<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="yjc.wdb.mapper.CopySoundsMapper">

<select id="AllMimiCryMenu" resultType="MimiCryMenu">
	select * from mimicrymenu;
</select>

<insert id="insertBoard">
	insert into board(b_title, b_content, b_voicefile, mm_no) values(#{b_title}, #{b_content}, #{b_voicefile}, #{mm_no});
</insert>

</mapper>